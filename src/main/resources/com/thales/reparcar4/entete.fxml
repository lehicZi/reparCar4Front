<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="94.0" fitWidth="155.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../images/reparcarLogo.jpg" />
         </image>
      </ImageView>
      <Label alignment="CENTER" prefHeight="90.0" prefWidth="222.0" text="ReparCar" textFill="#b41717">
         <font>
            <Font name="Britannic Bold" size="41.0" />
         </font>
      </Label>
      <Label fx:id="lbTitre" alignment="CENTER" prefHeight="90.0" prefWidth="374.0" text="???" textFill="#0d0d0d">
         <font>
            <Font name="Britannic Bold" size="41.0" />
         </font>
      </Label>
      <Pane prefHeight="17.0" prefWidth="200.0">
         <children>
            <Button fx:id="bntMenu" layoutY="21.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="79.0" text="Menu" textAlignment="CENTER" textFill="#b41717">
               <font>
                  <Font name="Britannic Bold" size="21.0" />
               </font></Button>
            <Button fx:id="bntdeCo" layoutX="95.0" layoutY="44.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="94.0" text="Deconnexion">
