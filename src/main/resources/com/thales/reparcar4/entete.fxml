            <Label fx:id="lbUser" layoutX="125.0" layoutY="21.0" text="Label" textFill="#b41717">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</HBox>
